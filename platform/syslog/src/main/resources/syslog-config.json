    "matchingGroupHost":6,
    "matchingGroupMessage":8,
    "parser":"org.opennms.netmgt.syslogd.RadixTreeSyslogParser",
    "discardUei":"DISCARD-MATCHING-MESSAGES",
    "queueSize":10000,
    "batchSize":1000,
    "batchInterval":500
  },
  "ueiMatches":[],
  "hideMatches":[],
  "importFiles":[]
}
