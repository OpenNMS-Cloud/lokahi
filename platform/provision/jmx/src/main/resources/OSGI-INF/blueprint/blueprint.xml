<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.osgi.org/xmlns/blueprint/v1.0.0 
		https://osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
">
    <reference id="jmxConfigDao" interface="org.opennms.horizon.jmx.dao.JmxConfigDao" />
    <reference id="jmxConnectionFactory" interface="org.opennms.horizon.jmx.connection.JMXConnectionFactory"/>
	<service interface="org.opennms.netmgt.provision.ServiceDetectorFactory">
        <bean class="org.opennms.horizon.provision.detector.jmx.Jsr160DetectorFactory" >
            <argument ref="jmxConfigDao" />
            <argument ref="jmxConnectionFactory" />
        </bean>
	</service>
</blueprint>
